#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    
    int i=0;
    int N;
    cin>>N;
    int reverse [N]={1,2,3,4,5};
    while(N>0)
    {
        cout<<reverse[N-1];
        N++;
    }
    
     
    return 0;
}
